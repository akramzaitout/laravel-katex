{
    "name": "akram-zaitout/laravel-katex",
    "description": "A comprehensive Laravel package for rendering beautiful mathematical expressions using KaTeX",
    "keywords": [
        "laravel",
        "katex",
        "math",
        "mathematics",
        "latex",
        "equations",
        "blade",
        "rendering"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Akram Zaitout",
            "email": "akramzaitout@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.5|^9.0|^10.0|^11.0",
        "phpstan/phpstan": "^1.10",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "AkramZaitout\\LaravelKatex\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AkramZaitout\\LaravelKatex\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AkramZaitout\\LaravelKatex\\KatexServiceProvider"
            ],
            "aliases": {
                "Katex": "AkramZaitout\\LaravelKatex\\Facades\\Katex"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.2.4"
}